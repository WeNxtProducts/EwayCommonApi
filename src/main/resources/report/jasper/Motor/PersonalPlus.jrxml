		</field>
		<group name="ROW_NUM">
			<groupExpression><![CDATA[$F{ROW_NUM}]]></groupExpression>
		</group>
		<group name="terms">
			<groupExpression><![CDATA[$F{terms}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="pvImagepath" class="java.lang.String"/>
	<parameter name="pvPolicyNo" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
HPM.POLICY_NO,
CONCAT_WS(".",PIF.TITLE_DESC,PIF.CLIENT_NAME) AS INSURED_NAME,
CONCAT_WS(" ",PIF.Address_1,PIF.Address_2) AS ADDRESS,
HPM.DEBIT_NOTE_NO AS INVOICE_NO,
UPPER(DATE_FORMAT(HPM.inception_date,"%d-%b-%y")) AS FROM_DATE,
UPPER(DATE_FORMAT(HPM.expiry_date,"%d-%b-%y")) AS TO_DATE,
UPPER(DATE_FORMAT(FROM_UNIXTIME(UNIX_TIMESTAMP(HPM.expiry_date) + (1*24*60*60)),"%d %b %Y")) AS RENEWAL_DATE,
DATE_FORMAT(CURDATE(),'%D-%b-%Y') AS PRINTED_DATE,
LUI.USER_NAME AS PREPARED_BY,
LUI.USER_NAME AS PRINTED_BY,
UPPER(DATE_FORMAT(CURDATE(),'%d-%b-%y')) AS FOOTER_DATE,
LBM.BRANCH_NAME AS PLACE,
LCM.COMPANY_NAME AS COMPANY_NAME,
CASE WHEN HPM.SECTION_ID='0' THEN 'DOMESTIC'
ELSE ( SELECT SECTION_NAME FROM PRODUCT_SECTION_MASTER PSD
WHERE 
HPM.COMPANY_ID=PSD.COMPANY_ID AND
PSD.STATUS='Y' AND
CURDATE() BETWEEN PSD.EFFECTIVE_DATE_START AND PSD.EFFECTIVE_DATE_END AND
HPM.PRODUCT_ID=PSD.PRODUCT_ID AND
HPM.SECTION_ID = PSD.SECTION_ID AND 
PSD.AMEND_ID = (SELECT MAX(AMEND_ID) FROM product_SECTION_MASTER PSD WHERE
HPM.COMPANY_ID=PSD.COMPANY_ID AND
PSD.STATUS='Y' AND
CURDATE() BETWEEN PSD.EFFECTIVE_DATE_START AND PSD.EFFECTIVE_DATE_END AND
HPM.PRODUCT_ID=PSD.PRODUCT_ID AND
HPM.SECTION_ID = PSD.SECTION_ID 
)
)
END AS  SECTION_NAME,
HPM.CURRENCY AS CURRENCY,
HPM.VAT_PERCENT,
CASE WHEN  HPM.SECTION_ID='0' THEN SUM(BUILDING_SUMINSURED) 
ELSE ( SELECT SUM(SUM_INSURED) FROM COMMON_DATA_DETAILS CDD WHERE CDD.QUOTE_NO=HPM.QUOTE_NO)
END AS SUMINSURED,
CASE WHEN (LCM.CURRENCY_ID=HPM.CURRENCY) THEN HPM.PREMIUM_LC ELSE HPM.PREMIUM_FC END AS PREMIUM,
CASE WHEN (hpm.currency = LCM.CURRENCY_ID) THEN HPM.VAT_PREMIUM_LC ELSE HPM.VAT_PREMIUM_FC END AS VAT,
CASE WHEN (hpm.currency = LCM.CURRENCY_ID) THEN HPM.OVERALL_PREMIUM_LC ELSE HPM.OVERALL_PREMIUM_FC END AS TOTAL,
LCM.SIGNATURE
FROM
HOME_POSITION_MASTER HPM,PERSONAL_INFO PIF,LOGIN_USER_INFO LUI,EWAY_LOGIN_BRANCH_MASTER LBM,
eway_insurance_company_master LCM,BUILDING_DETAILS BD
WHERE
HPM.CUSTOMER_ID = PIF.CUSTOMER_ID AND
HPM.LOGIN_ID=LUI.LOGIN_ID AND
HPM.LOGIN_ID=LBM.LOGIN_ID AND
HPM.COMPANY_ID=LBM.COMPANY_ID AND
HPM.BRANCH_CODE=LBM.BRANCH_CODE AND
LBM.STATUS='Y' AND
HPM.COMPANY_ID=LCM.COMPANY_ID AND
LCM.STATUS='Y' AND
CURDATE() BETWEEN LCM.EFFECTIVE_DATE_START AND LCM.EFFECTIVE_DATE_END AND
LCM.AMEND_ID= (SELECT MAX(AMEND_ID) FROM eway_insurance_company_master LCM WHERE 
HPM.COMPANY_ID=LCM.COMPANY_ID AND
LCM.STATUS='Y' AND
CURDATE() BETWEEN LCM.EFFECTIVE_DATE_START AND LCM.EFFECTIVE_DATE_END
) AND
HPM.QUOTE_NO=BD.QUOTE_NO AND
HPM.POLICY_NO=$P{pvPolicyNo}]]>
	</queryString>
